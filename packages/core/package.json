{
  "name": "neon-ui",
  "version": "0.0.1",
  "description": "NeonUI framework",
  "license": "MIT",
  "author": "smtdfc <me.smtdfc@gmail.com>",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "lib/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build:prod": "NODE_ENV=production rollup -c",
    "build:dev": "NODE_ENV=development rollup -c",
    "clean": "rm -rf dist",
    "test": "node ../../test/generator",
    "prepublishOnly": "cp ../../LICENSE . && npm run clean && npm run build:prod"
  },
  "keywords": [
    "compiler",
    "core",
    "framework",
    "neon-ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/smtdfc/neon-ui"
  },
  "bugs": {
    "url": "https://github.com/smtdfc/neon-ui/issues"
  },
  "homepage": "https://github.com/smtdfc/neon-ui/tree/main/packages/core#readme",
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "dependencies": {
    "@popperjs/core": "^2.11.8"
  }
}
